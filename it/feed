	elementorFrontend.hooks.addAction('frontend/element_ready/form.default', function() {
		var country = getCookie('country_code');
		var country_selection_country = country_selection;
		var dialling_code_selection = '';
		if (country) {
			country_selection_country = country_selection.replace('value="' + country + '"', 'value="' + country + '" selected');
		}
		dialling_code_selection = country_selection.replace(/data-dialling_code="([^"]*)"([^"]*)data-icon="([^"]*)"([^"]*)data-code="([^"]*)"([^"]*)value="([A-Z]+)">([^"]*)<\/option>/g, 'data-icon="$3" data-code_iso3="$7" value="$1">$1</option>');
		if (country) {
			dialling_code_selection = dialling_code_selection.replace('data-code_iso3="' + country + '"', 'data-code_iso3="' + country + '" selected');
		}
		if (disable_form_countries) {
			disable_form_countries.forEach(function(item) {
				country_selection_country = country_selection_country.replace('value="' + item + '"', 'value="' + item + '" disabled');
			});
		} else {
			country_selection_country = country_selection_country.replace(/data-promotions="([^"]*)"/g, '');
		}
		jQuery('#form-field-country').append(country_selection_country);
		jQuery('#form-field-dialling_code').append(dialling_code_selection);
		var optionFormat = function(item) {
			if (!item.id) {
				return item.text;
			}
			var span = document.createElement('span');
			var imgUrl = item.element.getAttribute('data-icon');
			var template = '';
			template += '<img decoding="async" src="' + imgUrl + '" class="country-icon" alt="image"/>';
			template += item.text;
			span.innerHTML = template;
			return jQuery(span);
		}
		jQuery('#form-field-country,#form-field-dialling_code').select2({
			templateSelection: optionFormat,
			templateResult: optionFormat
		});
		jQuery(document).on('change', '#form-field-country', function() {
			var dialling_code = jQuery("#form-field-country :selected").attr('data-dialling_code');
			if (dialling_code && jQuery('#form-field-dialling_code').length) {
				jQuery('#form-field-dialling_code option:selected').remove();
				jQuery('#form-field-dialling_code').val(dialling_code);
			}
			jQuery('.promotion-error').remove();
			jQuery('.promotion button[type="submit"]').prop('disabled', "").removeClass('disabled');
		});
		jQuery(document).on('submit_success', function() {
			if (jQuery('.hide_after_submit').length) {
				jQuery('.hide_after_submit .elementor-message').show();
				jQuery('.hide_after_submit .elementor-form-fields-wrapper').hide();
			}
		});
		jQuery(document).on('click', '.popup-close', function() {
			if (jQuery('.hide_after_submit').length) {
				jQuery('.hide_after_submit .elementor-form-fields-wrapper').show();
				jQuery('.hide_after_submit .elementor-message').hide();
			}
		});
		if (jQuery('#form-field-country').find(':selected').prop('disabled')) {
			jQuery('.elementor-field-group-country').append('<div class="promotion-error"><p>Promotion is not available in this country.</p></div>');
			jQuery('.promotion button[type="submit"]').prop('disabled', true).addClass('disabled');
		}
	});
});</script>		<form class="elementor-form" method="post" id="salesforce" name="Salesforce">
