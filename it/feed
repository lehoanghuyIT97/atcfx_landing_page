				<div class="elementor-widget-container">
			<script>
    jQuery(document).ready(function($){
        $('.trade-item-container.forex').click(function(){
            window.location.href="/it/mercati/forex-trading";
        });
        $('.trade-item-container.commodities').click(function(){
            window.location.href="/it/mercati/materie-prime";
        });
        $('.trade-item-container.share').click(function(){
            window.location.href="/it/mercati/trading-sulle-azioni";
        });
        $('.trade-item-container.indices').click(function(){
            window.location.href="/it/mercati/trading-sugli-indici";
        });
        $('.trade-item-container.crypto').click(function(){
