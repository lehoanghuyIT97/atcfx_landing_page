<script>
    jQuery(document).ready(function($){
        $("#btnMapAP").on("click",function(){
            if(!$(this).hasClass("active")){
                $(".imgMap").css("display","none");
                $(".mapGalleryContainer").css("display","none");
                $("#imgMapAP").css("display","block");
                $("#galleryContainerAP").css("display","block");
                $(".btnMap").removeClass("active");
                $(this).toggleClass("active");
            }
        });
        
        $("#btnMapAF").on("click",function(){
            if(!$(this).hasClass("active")){
                $(".imgMap").css("display","none");
                $(".mapGalleryContainer").css("display","none");
                $("#imgMapAF").css("display","block");
                $("#galleryContainerAF").css("display","block");
