						<div class="elementor-container elementor-column-gap-default">
					<div class="elementor-column elementor-col-100 elementor-inner-column elementor-element elementor-element-f9dd3bf" data-id="f9dd3bf" data-element_type="column">
			<div class="elementor-widget-wrap elementor-element-populated">
						<div class="elementor-element elementor-element-9682a9d elementor-widget elementor-widget-html" data-id="9682a9d" data-element_type="widget" data-widget_type="html.default">
				<div class="elementor-widget-container">
			<select name="mapTab-dropdown" id="mapTab-dropdown">
  <option id="dropdownMapAP" value="dropdownMapAP">Asia Pacifica</option>
  <option id="dropdownMapAF" value="dropdownMapAF">Africa</option>
  <option id="dropdownMapEU" value="dropdownMapEU">Europa</option>
  <option id="dropdownMapLT" value="dropdownMapLT">America Latina</option>
  <option id="dropdownMapME" value="dropdownMapME">Medio Oriente</option>
</select>

<script>
    (function ($) {
        $(document).ready(function () {
          $('#mapTab-dropdown').change(function() {
            $selectedValue = $(this).val();
            console.log($selectedValue);
            
            if ($selectedValue == "dropdownMapAP") {
                $(".mapGalleryContainer").css("display","none");
                $("#galleryContainerAP").css("display","block");
                $(".btnMap").removeClass("active");
                $(this).toggleClass("active");
            }
            
            if ($selectedValue == "dropdownMapAF") {
                $(".mapGalleryContainer").css("display","none");
                $("#galleryContainerAF").css("display","block");
                $(".btnMap").removeClass("active");
                $(this).toggleClass("active");
            }
            
            if ($selectedValue == "dropdownMapEU") {
                $(".mapGalleryContainer").css("display","none");
                $("#galleryContainerEU").css("display","block");
                $(".btnMap").removeClass("active");
                $(this).toggleClass("active");
            }
            
            if ($selectedValue == "dropdownMapLT") {
                $(".mapGalleryContainer").css("display","none");
                $("#galleryContainerLT").css("display","block");
                $(".btnMap").removeClass("active");
                $(this).toggleClass("active");
            }
            
            if ($selectedValue == "dropdownMapME") {
                $(".mapGalleryContainer").css("display","none");
                $("#galleryContainerME").css("display","block");
                $(".btnMap").removeClass("active");
                $(this).toggleClass("active");
            }
            
            
          });
        })
    })(jQuery);
</script>
		</div>
				</div>
					</div>
		</div>
					</div>
		</section>
